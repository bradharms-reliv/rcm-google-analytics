{
  "name": "rcm/google-analytics",
  "description": "Rcm Google Analytics",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "rcm",
    "reliv",
    "module",
    "zf2",
    "google",
    "analytics"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/reliv/rcm-google-analytics",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "https://github.com/reliv"
    }
  ],
  "require": {
    "php": ">=5.4",
    "zendframework/zendframework": "2.*",
    "doctrine/doctrine-orm-module": "0.* || 1.*",
    "rcm/core": "^1.20.0",
    "rcm/core-js": "^1.0.0",
    "rcm/user": "1.*",
    "rcm/i18n": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Reliv\\RcmGoogleAnalytics\\": "src/"
    }
  },
  "extra": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-installer-paths": {
      "bower-asset-library": "public/bower_components"
    }
  },
  "config": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-installer-paths": {
      "bower-asset-library": "public/bower_components"
    }
  }
}
